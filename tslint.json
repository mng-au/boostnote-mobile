{
  "extends": ["tslint:latest", "tslint-react"],
  "rules": {
    "jsx-wrap-multiline": false,
    "jsx-no-multiline-js": false,
    "quotemark": [ true, "single" ],
    "member-access": false,
    "no-console": false,
    "ordered-imports": false,
    "object-literal-sort-keys": false,
    "object-literal-shorthand": false
  }
}
